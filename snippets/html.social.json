{
  "GitHub followers": {
    "prefix": "svg-github-followers",
    "body": "<a href=\"${5:https://github.com/${1:user}/${2:repository}}\"><img src=\"https://img.shields.io/github/followers/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}\" title=\"GitHub\" alt=\"GitHub\" /></a>$9"
  },
  "GitHub forks": {
    "prefix": "svg-github-forks",
    "body": "<a href=\"${5:https://github.com/${1:user}/${2:repository}}\"><img src=\"https://img.shields.io/github/forks/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}\" title=\"GitHub\" alt=\"GitHub\" /></a>$9"
  },
  "GitHub stars": {
    "prefix": "svg-github-stars",
    "body": "<a href=\"${5:https://github.com/${1:user}/${2:repository}}\"><img src=\"https://img.shields.io/github/stars/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}\" title=\"GitHub\" alt=\"GitHub\" /></a>$9"
  },
  "Twitter follow": {
    "prefix": "svg-twitter-follow",
    "body": "<a href=\"http://twitter.com/${1:username}\"><img src=\"https://img.shields.io/badge/https://img.shields.io/badge/Follow-%40${1:username}-${2:blue}.svg${3:&amp;style=flat${4:-square}}\" title=\"Twitter\" alt=\"Twitter\" /></a>$9"
  },
  "Twitter URL": {
    "prefix": "svg-twitter-url",
    "body": "<a href=\"$5\"><img src=\"https://img.shields.io/twitter/url/https/${1:url}.svg${3:?style=flat${4:-square}}\" title=\"Twitter\" alt=\"Twitter\" /></a>$9"
  }
